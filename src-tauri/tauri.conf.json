{
  "productName": "decent-client",
  "version": "0.1.0",
  "identifier": "gg.decent-client.dev",
  "build": {
    "frontendDist": "../build/client",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build"
  },
  "app": {
    "windows": [
      {
        "title": "Decent Client",
        "width": 400,
        "height": 200,
        "center": true,
        "resizable": false,
        "transparent": true,
        "decorations": false,
        "visible": true,
        "label": "splash-screen"
      },
      {
        "title": "Decent Client | Launcher",
        "width": 1086,
        "height": 548,
        "minWidth": 792,
        "minHeight": 540,
        "center": true,
        "resizable": true,
        "maximizable": false,
        "transparent": true,
        "decorations": false,
        "visible": false,
        "label": "main-launcher",
        "url": "launcher"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json"
}
